{
  "name": "express-umi-project-skeleton",
  "version": "1.0.0",
  "description": "Using express to solve cross domain problems, web base on umi3.0.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "nodemon ./server/bin/www",
    "dev": "umi dev",
    "umiBuild": "umi build",
    "build": "sh build.sh",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/preset-react": "1.x",
    "axios": "^0.21.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "request": "^2.88.2",
    "umi": "^3.3.7"
  },
  "devDependencies": {
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@umijs/test": "^3.3.7",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tennesseesunshine/express-umi-project-skeleton.git"
  },
  "keywords": [
    "umi3.0",
    "express",
    "cross",
    "node-proxy"
  ],
  "author": "yangsen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tennesseesunshine/express-umi-project-skeleton/issues"
  },
  "homepage": "https://github.com/Tennesseesunshine/express-umi-project-skeleton#readme"
}